<?xml version='1.0' encoding='utf-8'?>
<widget id="com.smart.droidies.hindi.tips" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SpinnerDialog">
        <param name="android-package" value="hu.dpal.phonegap.plugins.SpinnerDialog" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="AdmobPlugin">
        <param name="android-package" value="com.admob.cordova.plugin.AdmobAdPlugin" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="EmailComposer">
        <param name="android-package" value="de.appplant.cordova.emailcomposer.EmailComposer" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <allow-intent href="market:*" />
    <icon density="mdpi" src="res/android/hindi_tips_mdpi.png" />
    <icon density="hdpi" src="res/android/hindi_tips_hdpi.png" />
    <icon density="xhdpi" src="res/android/hindi_tips_xhdpi.png" />
    <icon density="xxhdpi" src="res/android/hindi_tips_xxhdpi.png" />
    <icon density="xxxhdpi" src="res/android/hindi_tips_xxxhdpi.png" />
    <name>Hindi Tips</name>
    <description>
        Apache Cordova application for Hindi Tips
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
